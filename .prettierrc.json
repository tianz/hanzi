{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "tabWidth": 2,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": ["pages/(.*)$", "components/(.*)$", "lib/(.*)$", ".css$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
